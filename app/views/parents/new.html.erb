<h1>Add Parent</h1>
<%= form_for([@baby, @parent]) do |f| %>
	<div class="field"><%= f.label :username,"User name" %> <br> <%= f.text_field :username %></div>
	<p></p>
	<div class="field"><%= f.label :password %> <br> <%= f.password_field :password %></div>
	<p></p>
	<div class="actions"><%= f.submit "Add" %></div>
<% end %>